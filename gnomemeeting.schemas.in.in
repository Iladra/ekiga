<!-- Remeber to increment SCHEMA_AGE in configure.in if you add any new
     schema to this file -->
<gconfschemafile>
  <schemalist>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/audio_manager</key>
      <applyto>/apps/gnomemeeting/devices/audio_manager</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>ALSA</default>
      <locale name="C">
	<short>Audio manager</short>
	<long>The audio manager that will be used to detect the devices and manage them.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/video_manager</key>
      <applyto>/apps/gnomemeeting/devices/video_manager</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>V4L</default>
      <locale name="C">
	<short>Video manager</short>
	<long>The video manager that will be used to detect the devices and manage them.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/video_recorder</key>
      <applyto>/apps/gnomemeeting/devices/video_recorder</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>/dev/video0</default>
      <locale name="C">
	<short>Video device</short>
	<long>Select the video device to use. Using an invalid video device or "Picture" for video transmission will transmit a test picture.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/view/show_status_bar</key>
      <applyto>/apps/gnomemeeting/view/show_status_bar</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Show status bar</short>
	<long>Show a status bar at the bottom of the main window</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/view/show_chat_window</key>
      <applyto>/apps/gnomemeeting/view/show_chat_window</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Show chat window</short>
	<long>Show the chat window in the main window</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/view/control_panel_section</key>
      <applyto>/apps/gnomemeeting/view/control_panel_section</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>4</default>
      <locale name="C">
	<short>Change the control panel section</short>
	<long>Change the control panel section (4 means that the control panel is off)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/view/show_popup</key>
      <applyto>/apps/gnomemeeting/view/show_popup</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Display a popup window when receiving a call</short>
	<long>If enabled, a popup window will be displayed when receiving an incoming call.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/view/show_splash</key>
      <applyto>/apps/gnomemeeting/view/show_splash</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Show splash screen</short>
	<long>If enabled, the splash screen will be displayed when GnomeMeeting starts.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/video_settings/tr_fps</key>
      <applyto>/apps/gnomemeeting/video_settings/tr_fps</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>30</default>
      <locale name="C">
	<short>Minimum transmitted FPS</short>
	<long>The minimum number of video frames to transmit each second when trying to minimize the bandwidth.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/video_settings/max_video_bandwidth</key>
      <applyto>/apps/gnomemeeting/video_settings/max_video_bandwidth</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>6</default>
      <locale name="C">
	<short>Maximum video bandwidth</short>
	<long>The maximum video bandwidth in kbytes/s. The video quality and the number of transmitted frames per second will be dynamically adjusted above their minimum during calls to try to minimize the bandwidth to the given value.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/video_settings/tr_vq</key>
      <applyto>/apps/gnomemeeting/video_settings/tr_vq</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>75</default>
      <locale name="C">
	<short>Minimum transmitted video quality</short>
	<long>The minimum transmitted video quality to keep when trying to minimize the used bandwidth:  choose 100% on a LAN for the best quality, 1% being the worst quality.</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/personal_data/firstname</key>
      <applyto>/apps/gnomemeeting/personal_data/firstname</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>First name</short>
	<long>Enter your first name.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/personal_data/lastname</key>
      <applyto>/apps/gnomemeeting/personal_data/lastname</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Last name</short>
	<long>Enter your last name.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/personal_data/comment</key>
      <applyto>/apps/gnomemeeting/personal_data/comment</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Comment</short>
	<long>Enter a comment about yourself for the user directory.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/personal_data/location</key>
      <applyto>/apps/gnomemeeting/personal_data/location</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Location</short>
	<long>Enter your location (country or city) for the user directory.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/personal_data/mail</key>
      <applyto>/apps/gnomemeeting/personal_data/mail</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>E-mail address</short>
	<long>Enter your e-mail address.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/audio_codecs/codecs_list</key>
      <applyto>/apps/gnomemeeting/audio_codecs/codecs_list</applyto>
      <owner>GnomeMeeting</owner>
      <type>list</type>
      <list_type>string</list_type>
      <default>[MS-GSM=1,GSM-06.10=1,G.726-32k=1,G.711-uLaw-64k=1,G.711-ALaw-64k=1,LPC-10=1,G.723.1=0]</default>
      <locale name="C">
	<short>The Audio Codecs List</short>
	<long>The Audio Codecs List</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/audio_settings/gsm_frames</key>
      <applyto>/apps/gnomemeeting/audio_settings/gsm_frames</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>4</default>
      <locale name="C">
	<short>Number of frames for GSM</short>
	<long>Number of frames transmitted in each packet for the GSM codec.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/audio_settings/g711_frames</key>
      <applyto>/apps/gnomemeeting/audio_settings/g711_frames</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>30</default>
      <locale name="C">
	<short>Number of frames for G.711</short>
	<long>Number of frames transmitted in each packet for the G.711 codec.</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/audio_settings/sd</key>
      <applyto>/apps/gnomemeeting/audio_settings/sd</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Enable silence detection</short>
	<long>If enabled, use silence detection with the GSM and G.711 codecs.</long>
      </locale>
    </schema>
    <schema>		
      <key>/schemas/apps/gnomemeeting/ldap/ldap_server</key>
      <applyto>/apps/gnomemeeting/ldap/ldap_server</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>ils.seconix.com</default>
      <locale name="C">
	<short>User directory</short>
	<long>The user directory server to register with.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/ldap/register</key>
      <applyto>/apps/gnomemeeting/ldap/register</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Enable registering</short>
	<long>If enabled, register with the selected user directory.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/ldap/visible</key>
      <applyto>/apps/gnomemeeting/ldap/visible</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Show my details to other registered users</short>
	<long>If enabled, your details are shown to people browsing the user directory. If disabled, you are not visible to users browsing the user directory, but they can still use the callto URL to call you.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/auto_answer</key>
      <applyto>/apps/gnomemeeting/general/auto_answer</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Automatically answer calls</short>
	<long>If enabled, incoming calls will be automatically answered.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/save_prefix</key>
      <applyto>/apps/gnomemeeting/general/save_prefix</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>gnomemeeting-snap-</default>
      <locale name="C">
	<short>The save prefix</short>
	<long>The prefix that will be used to save pictures of the conversation. That prefix is followed by the snapshot number.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/version</key>
      <applyto>/apps/gnomemeeting/general/version</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>9</default>
      <locale name="C">
	<short>Version</short>
	<long>Version</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/do_not_disturb</key>
      <applyto>/apps/gnomemeeting/general/do_not_disturb</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Do not disturb</short>
	<long>If enabled, incoming calls will be automatically refused.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/auto_clear_text_chat</key>
      <applyto>/apps/gnomemeeting/general/auto_clear_text_chat</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Automatically clear the text chat at the end of calls</short>
	<long>If enabled, the text chat will automatically be cleared at the end of calls.</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/general/gconf_test_age</key>
      <applyto>/apps/gnomemeeting/general/gconf_test_age</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>@SCHEMA_AGE@</default>
      <locale name="C">
	<short>Revision of the schema file</short>
	<long>Revision of the schema file</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/ports/listen_port</key>
      <applyto>/apps/gnomemeeting/ports/listen_port</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>1720</default>
      <locale name="C">
	<short>Listen Port</short>
	<long>The port to listen to for incoming connections. GnomeMeeting needs to be restarted for the new value to take effect.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/ports/listen_port</key>
      <applyto>/apps/gnomemeeting/ports/listen_port</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>1720</default>
      <locale name="C">
	<short>Listen Port</short>
	<long>The port to listen to for incoming connections. GnomeMeeting needs to be restarted for the new value to take effect.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/ports/tcp_port_range</key>
      <applyto>/apps/gnomemeeting/ports/tcp_port_range</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>30000:30010</default>
      <locale name="C">
	<short>TCP Port Range</short>
	<long>The range of TCP ports beside the listen_port that GnomeMeeting will use for the H.245 channel.  You can change the TCP port range if you are using IP translation and if you need to have several GnomeMeeting running behind your NAT/PAT router. GnomeMeeting needs to be restarted for the new values to take effect. This port range has no effect if both participants to the conference are using H.245 Tunneling.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/ports/rtp_port_range</key>
      <applyto>/apps/gnomemeeting/ports/rtp_port_range</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>5000:5003</default>
      <locale name="C">
	<short>UDP Port Range</short>
	<long>The range of UDP ports that GnomeMeeting will use for RTP (audio and video communication channels). You can change the UDP port range if you are using IP translation and if you need to have several GnomeMeeting running behind your NAT/PAT router. GnomeMeeting needs to be restarted for the new values to take effect.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/ports/udp_port_range</key>
      <applyto>/apps/gnomemeeting/ports/udp_port_range</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>5010:5013</default>
      <locale name="C">
	<short>UDP Port Range</short>
	<long>The range of UDP ports that GnomeMeeting will use when registering to gatekeepers. You can change the UDP port range if you are using IP translation and if you need to have several GnomeMeeting running behind your NAT/PAT router. GnomeMeeting needs to be restarted for the new values to take effect. This port range has no effect if you are not registering to a gatekeeper.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/h245_tunneling</key>
      <applyto>/apps/gnomemeeting/general/h245_tunneling</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Enable H.245 tunnelling</short>
	<long>This enables H.245 Tunnelling mode. In H.245 Tunnelling mode H.245 messages are encapsulated into the the H.225 channel (port 1720). This saves one TCP connection during calls. H.245 Tunnelling was introduced in H.323v2 and Netmeeting does not support it. Using both Fast Start and H.245 Tunnelling can crash some versions of Netmeeting.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/fast_start</key>
      <applyto>/apps/gnomemeeting/general/fast_start</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Enable 'Fast Start'</short>
	<long>Connection will be established in Fast Start mode. Fast Start is a new way to start calls faster that was introduced in H.323v2. It is not supported by Netmeeting and using both Fast Start and H.245 Tunnelling can crash some versions of Netmeeting.</long>
      </locale>
    </schema>
   <schema>
      <key>/schemas/apps/gnomemeeting/general/ip_translation</key>
      <applyto>/apps/gnomemeeting/general/ip_translation</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Enable IP translation</short>
	<long>This enables IP translation. IP translation is useful if GnomeMeeting is running behind a NAT/PAT router. You have to put the public IP of the router in the field below. If you are registered to ils.seconix.com, GnomeMeeting will automatically fetch the public IP using the ILS service. If your router natively supports H.323, you can disable this.</long>
      </locale>
   </schema>
   <schema>
      <key>/schemas/apps/gnomemeeting/general/user_input_capability</key>
      <applyto>/apps/gnomemeeting/general/user_input_capability</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>0</default>
      <locale name="C">
	<short>User Input Capabilities type</short>
	<long>This permits to set the mode for User Input Capabilities. The values can be "All" (0), "None" (1), "rfc2833" (2), "Signal" (3) or "String" (4) (default is "All"). Choosing other values than "All", "String" or "rfc2833" disables the Text Chat.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/public_ip</key>
      <applyto>/apps/gnomemeeting/general/public_ip</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default></default>
      <locale name="C">
	<short>Public IP of the NAT/PAT router</short>
	<long>Enter the public IP of your NAT/PAT router if you want to use IP translation. If you are registered to ils.seconix.com, GnomeMeeting will automatically fetch the public IP using the ILS service.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/general/incoming_call_sound</key>
      <applyto>/apps/gnomemeeting/general/incoming_call_sound</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Play a sound on incoming calls</short>
	<long>If enabled, GnomeMeeting will play a sound when receiving an incoming call (the sound to play is chosen in the Gnome Control Center).</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/video_display/fullscreen</key>
      <applyto>/apps/gnomemeeting/video_display/fullscreen</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Switch to fullscreen</short>
	<long>Switch to fullscreen for images displayed in the main GUI.</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/video_display/zoom_factor</key>
      <applyto>/apps/gnomemeeting/video_display/zoom_factor</applyto>
      <owner>GnomeMeeting</owner>
      <type>float</type>
      <default>1.0</default>
      <locale name="C">
	<short>The zoom value</short>
	<long>The zoom value to apply to images displayed in the main GUI (can be 0.50, 1.00, or 2.00).</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/video_display/bilinear_filtering</key>
      <applyto>/apps/gnomemeeting/video_display/bilinear_filtering</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>false</default>
      <locale name="C">
	<short>Enable bilinear filtering on displayed video</short>
	<long>Enable or disable bilinear interpolation when rendering video images (this has no effect in fullscreen mode).</long>
      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/video_display/stay_on_top</key>
      <applyto>/apps/gnomemeeting/video_display/stay_on_top</applyto>
      <owner>GnomeMeeting</owner>
      <type>bool</type>
      <default>true</default>
      <locale name="C">
	<short>Place windows displaying video above other windows during calls.</short>
	<long>Place windows displaying video above other windows during calls.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/video_display/fullscreen_width</key>
      <applyto>/apps/gnomemeeting/video_display/fullscreen_width</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>640</default>
      <locale name="C">
	<short>Fullscreen width</short>
	<long>The image width for fullscreen.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/video_display/fullscreen_height</key>
      <applyto>/apps/gnomemeeting/video_display/fullscreen_height</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>480</default>
      <locale name="C">
	<short>Fullscreen height</short>
	<long>The image height for fullscreen.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/video_display/video_view</key>
      <applyto>/apps/gnomemeeting/video_display/video_view</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>1</default>
      <locale name="C">
	<short>The default video view</short>
	<long>The default video view (0: Local, 1: Remote, 2: Both incrusted, 3: Both with local video in a separate window, 4: Both)</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/video_display/local_video_width</key>
      <applyto>/apps/gnomemeeting/video_display/local_video_width</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>176</default>
      <locale name="C">
	<short>Local video window width</short>
	<long>The width of the local video window</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/video_display/local_video_height</key>
      <applyto>/apps/gnomemeeting/video_display/local_video_height</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>144</default>
      <locale name="C">
	<short>Local video window height</short>
	<long>The height of the local video window</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/video_display/remote_video_width</key>
      <applyto>/apps/gnomemeeting/video_display/remote_video_width</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>176</default>
      <locale name="C">
	<short>Remote video window width</short>
	<long>The width of the remote video window</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/video_display/remote_video_height</key>
      <applyto>/apps/gnomemeeting/video_display/remote_video_height</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>144</default>
      <locale name="C">
	<short>Remote video window height</short>
	<long>The height of the remote video window</long>
      </locale>
    </schema>
