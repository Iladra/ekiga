    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/audio_settings/max_jitter_buffer</key>
      <applyto>/apps/gnomemeeting/audio_settings/max_jitter_buffer</applyto>
      <owner>GnomeMeeting</owner>
      <type>int</type>
      <default>500</default>
