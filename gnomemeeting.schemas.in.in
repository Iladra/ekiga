      </locale>
    </schema>
    <schema>	
      <key>/schemas/apps/gnomemeeting/devices/lid_country</key>
      <applyto>/apps/gnomemeeting/devices/lid_country</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>BE</default>
      <locale name="C">
	<short>Country code</short>
	<long>The two-letter country code of your country (e.g.: BE, UK, FR, DE, ...).</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/audio_player_mixer</key>
      <applyto>/apps/gnomemeeting/devices/audio_player_mixer</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
      <default>/dev/mixer</default>
      <locale name="C">
	<short>Audio player mixer</short>
	<long>The mixer to use to change the volume of the audio player.</long>
      </locale>
    </schema>
    <schema>
      <key>/schemas/apps/gnomemeeting/devices/audio_recorder_mixer</key>
      <applyto>/apps/gnomemeeting/devices/audio_recorder_mixer</applyto>
      <owner>GnomeMeeting</owner>
      <type>string</type>
